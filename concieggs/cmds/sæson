#!/bin/sh

# This code was generated by ChatGPT

# determine the current month
month=$(date +%m)

# use a case statement to check the current month
# and set the season variable accordingly
case $month in
  12|1|2) season="vinter" ;;
  3|4|5) season="forår" ;;
  6|7|8) season="sommer" ;;
  9|10|11) season="efterår" ;;
esac

# print the current season
echo "Den nuværende sæson er $season"
