#!/bin/sh

if isKing "$EGGS_USER"; then
    echo "Som kongen ønsker"
    echo "Javel, Deres majestæt"
    echo "Det skal blive sådan, Deres højhed"
    echo "Deres royale ønske er min lov"
    echo "I kongens navn"
    echo "Jeg skal kundgøre Deres royale vilje for almuen, Deres majestæt"
    echo "$EGGS_USER længe leve"
    echo "Med ære"
    echo "Jeg takker Deres Højhed for at skænke mig dette glorværdige hverv"
else
    echo "ACK-ACK"
    echo "Javel"
    echo "OK"
    echo "A-OK"
    echo "Forstået"
    echo "Accepteret"
    echo "Det er i orden"
    echo "Roger"
    echo "10-4"
    echo "Den er fin"
    echo "Den er fino"
    echo "Den er fin-fin"
    echo "Den er fjong"
    echo "Jep, det siger vi"
    echo "Jep, det siger vi bare"
    echo "Det er dandy"
    echo "EXIT_SUCCESS"
    echo "Helt fedt, mand"
    echo "Tip top"
    echo "Godtaget"
    echo "Den er jeg med på"
    echo "Top dollar"
    echo "Fjong-fililong-gong-gong"
    echo "Det' bår' dæjlig"
    echo "Superduper"
    echo "Dupersuper"
fi | randomLine | xargs echo -n
(echo "!"; echo ".") | randomLine
